show databases;
create database harinabuena;
use harinabuena;
select database();
show tables;
create table harinabuena_tabla( 
	Mercancia text,
	Cantidad numeric,
    Empiezo year);
desc harinabuena_tabla;
insert into harinabuena_tabla(Mercancia,Cantidad,Empiezo) values('marihuana',200,2004);
select * from harinabuena_tabla;
drop table harinabuena_tabla;
drop database harinabuena;
